{
  "compilerOptions": {
    "outDir": "dist/functions",
    "rootDir": "src-functions",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmitOnError": true
  },
  "include": ["src-functions"]
}